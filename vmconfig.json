{
  "machine-config": {
    "vcpu_count": 2,
    "mem_size_mib": 1024
  },
  "boot-source": {
    "kernel_image_path": "vmlinux510.bin",
    "boot_args": "console=ttyS0 reboot=k panic=1 pci=off init=/init-agent ip=10.0.0.3::10.0.0.1:255.255.255.0::eth0:off"
  },
  "drives": [
    {
      "drive_id": "rootfs",
      "path_on_host": "rootfs.img",
      "is_root_device": true,
      "is_read_only": false
    }
  ],
  "network-interfaces": [
    {
      "iface_id": "eth0",
      "guest_mac": "AA:FC:00:00:00:01",
      "host_dev_name": "tap0"
    }
  ],
  "mmds": {
    "config": {
      "network_interfaces": ["eth0"],
      "version": "V1",
      "ipv4_address": "169.254.170.2"
    }
  }
}
